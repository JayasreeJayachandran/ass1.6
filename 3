import java.util.*;
 class Acad
 {
   public static void main(String args[])
   {
    Scanner s=new Scanner(System.in);
    int a=s.nextInt();
    int b=s.nextInt();
    int sum=add(a,b);
    System.out.println(sum);
    }
    public static int add(int a,int b)
    {
     int ad;
     ad=a+b;
     return ad;
     }
  }
